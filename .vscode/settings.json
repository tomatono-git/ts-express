// Place your settings in this file to overwrite default and user settings.
{
    "tslint.exclude": "**/node_modules/**/*",
    "window.zoomLevel": 0,
    // "terminal.integrated.shell.windows": "C:\\WINDOWS\\Sysnative\\bash.exe",
    // "terminal.integrated.shell.windows": "C:\\WINDOWS\\Sysnative\\WindowsPowerShell\\v1.0\\powershell.exe",
    "terminal.integrated.shell.windows": "C:\\Program Files\\Git\\bin\\bash.exe",
    // "git.path": "C:/Program Files/Git/bin/git.exe",
    // "window.menuBarVisibility": "default",
    "files.exclude": {
        "**/*.bak": true,
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true
    },
    "editor.renderWhitespace": "boundary",
    "editor.renderControlCharacters": true,
    "search.exclude": {
        "**/node_modules": true,
        "**/bower_components": true,
        "**/typings": true,
        "**/dist": true,
        "**/coverge": true
    },
    "editor.wordWrap": "on",
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "files.trimTrailingWhitespace": true,
    "window.title": "${activeEditorShort}${separator}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "*:/bower_components/*/**": true,
        "**/node_modules/*/**": true
    },
    "css.lint.duplicateProperties": "warning",
    "css.lint.important": "warning",
    "explorer.confirmDragAndDrop": false,
    "gitlens.advanced.messages": {
        "suppressShowKeyBindingsNotice": true
    },
    "explorer.confirmDelete": false,
    "editor.detectIndentation": false,
    "tslint.rulesDirectory": "./node_modules/codelyzer",
    "typescript.tsdk": "node_modules/typescript/lib",
    "workbench.sideBar.location": "left",
    "terminal.integrated.scrollback": 10000,
    "gitlens.gitExplorer.files.layout": "list",
    "git.autofetch": true,
    "css.lint.idSelector": "warning",
    "less.lint.idSelector": "warning",
    "scss.lint.idSelector": "warning",
    "css.lint.float": "warning",
    "css.lint.ieHack": "warning",
    "less.lint.float": "warning",
    "scss.lint.float": "warning",
    "scss.lint.ieHack": "warning",
    "css.lint.zeroUnits": "warning"
}